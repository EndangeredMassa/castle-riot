Game = require "game"

window.admin = window.location.search == '?admin'

$ ->
  src = 'http://nodeknockout.com/iframe/third-times-the-char'
  $('#nko-vote').attr('src', src)

# provide a way to get the delta-time since
# the last tick; not provided any other way
# that I can find!
jaws.dt = ->
  jaws.game_loop.tick_duration / 100

jaws.onload = ->
  jaws.unpack()
  jaws.assets.add Game.assets
  jaws.start Game

